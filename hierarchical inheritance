class Vehical {
    Vehical() {
        System.out.println("This is a Vehical");
    }
}
class Car extends Vehical {
    Car() {
        System.out.println("This is a Car");
    }
}
class Bus extends Vehical {
    Bus() {
        System.out.println("This is a Bus");
    }
}
public class Main {
    public static void Main(String[] args) {
        Car obj1=new Car();
        Bus obj2=new Bus();
        }
}
