public class Main {
    public static void main(String[] args) {
        int num = 1;
        int rows = 6;
        for (int i = 1; i <= rows; i++) {   
            for (int j = 1; j <= i; j++) {
                int square = num * num;
                System.out.print((square % 2) + " ");
                num++;
            }
            System.out.println();
        }
    }
}
