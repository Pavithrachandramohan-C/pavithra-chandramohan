import java.util.Scanner;
class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter a string: ");
        String x = sc.nextLine();
        String y = "[\\.\\s]";
        String[] z = x.split(y);
        for (String s : z) {
            System.out.println(s);
        }
    }
}
