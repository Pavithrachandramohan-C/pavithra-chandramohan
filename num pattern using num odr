public class Main {
    public static void main(String[] args) {
        int num=1;
        int rows = 6;
        for (int i = rows; i >=1; i--) { 
            for (int j = i; j >= 1; j--) { 
                System.out.print( num + " ");
                num++;
            }
            System.out.println(); 
        }
    }
}
